#include <stdio.h>

int main() {
    int h,m,n;//변수선언
    scanf("%d %d",&h ,&m);//시간,분을 스켄
    n = h * 60 + m;// n에 시간*60을 한 후 분을 더한값을 대입
    n = n + (60 * 24);//n에 24시간 더함:0 0이 들어왔을 때 음수가 되기 때문
    n -= 30;//n의 값에 30을 뺌
    
    printf("%d %d", (n/60)%24,n%60);//시간(n)을 60으로 나눈 후 24를 나머지연산을 함,그리고 분을 구해야 함으로 나머지 연산을 한다.
    
    return 0;
}
