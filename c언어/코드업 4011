#include <stdio.h>

int main() {
    char s[14];
    scanf("%s", s);
    printf("%c%c%c%c/%c%c/%c%c %c",
            s[7] == '1' || s[7] == '2' ? '1' : '2',
            s[7] == '1' || s[7] == '2' ? '9' : '0',
            s[0], s[1],
            s[2], s[3],
            s[4], s[5],
            s[7] == '1' || s[7] == '3' ? 'M' : 'F');
    return 0;
}
