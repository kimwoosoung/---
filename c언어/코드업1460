#include <stdio.h>

int main(){
    int arr[100][100];
    int num;
    int a=1;
    scanf("%d", &num);
    for(int i=0; i<num; i++){
        for(int j=0; j<num; j++){
            arr[i][j]=a;
            a++;
        }
    }
    
    for(int i=0; i<num; i++){
        for(int j=0; j<num; j++){
            printf("%d ",arr[i][j]);
        }
        printf("\n");
    }
    
    return 0;
}
